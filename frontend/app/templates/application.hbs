<div class="container-main">
  <div class="layout-row layout-wrap">
    <div class="flex-grow" style="background-color:#5e3c58;">
      <h1> <a target="_blank" href="{{constants.rootURL}}img/logo.png">
        <img src="{{constants.rootURL}}img/logo.png" width="100" height="50">
      </a>
        Pilgrim Lutheran Church</h1>
    </div>
  </div>
  <div class="navbar">
    {{nav-bar}}
  </div>
  <div class="body">
    {{liquid-outlet "main"}}
	</div>
  <div class="footer" bgcolor="#000000">
    <div class="layout-row layout-md layout-wrap">
      <div class="flex-25%">
        <h2>Facebook</h2>
        {{facebook-feed}}
      </div>
      <div class="flex-15% flex-noshrink">
        <div align="center">
          <h2>Sermons & Messages</h2>
          {{#each model as |sermon|}}
          <a target="_blank" href="{{sermon.document}}">
            <h3><li>{{sermon.description}}</li></h3>
          </a>
          {{/each}}
        </div>
      </div>
      <div class="flex-30 googleMap">
        <div>
        <h2>Location</h2>
        <p>2311 Fairview Rd, Bellevue, NE 68123</p>
        <iframe

          frameborder="0" style="border:0; width: auto; height:auto;"
          src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBGfC4IpmxJmeseLAOXjtCwKafZQXy7HbY&q=Pilgrim+Lutheran+Church,Bellevue+NE" allowfullscreen>
        </iframe>
        </div>
      </div>
    </div>
    <div class="layout-row">
      <div class="flex">Â© 2016 - Pilgrim Lutheran Church</div>
    </div>
  </div>




</div>
